<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.2.1.2</Version>
    <Description>ReadAllText, WriteAllText etc. with some magic ability</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoBts" Version="24.2.1.1" />
    <PackageReference Include="SunamoCollectionsGeneric" Version="24.2.1.1" />
    <PackageReference Include="SunamoConverters" Version="24.2.1.1" />
    <PackageReference Include="SunamoFileSystem" Version="24.2.1.1" />
    <PackageReference Include="SunamoSerializer" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringGetLines" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringReplace" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringShared" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringSplit" Version="24.2.1.1" />
    <PackageReference Include="SunamoPlatformUwpInterop" Version="24.2.1.1" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>